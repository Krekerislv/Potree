{
  "name": "potree",
  "private": true,
  "version": "1.8.0",
  "description": "WebGL point cloud viewer",
  "keywords": [
    "point",
    "cloud",
    "pointcloud",
    "octree",
    "entwine",
    "viewer",
    "threejs",
    "webgl",
    "browser",
    "tool"
  ],
  "main": "./build/potree/potree.js",
  "scripts": {
    "start": "gulp watch 2>&1 | while IFS= read -r line; do echo \"[$(date '+%Y-%m-%d %H:%M:%S')] $line\" >> ./potree_log.log; echo $line; done",
    "build": "gulp build pack",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "express": "*",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-connect": "^5.7.0",
    "json5": "^2.1.3",
    "python-shell": "*",
    "rollup": "^1.31.1",
    "socket.io": "*",
    "through": "~2.3.4"
  },
  "author": "Markus Sch√ºtz",
  "license": "BSD-2-CLAUSE"
}
